//	printf("%d\n",(10>5) || (8<3));
//	printf("%d\n", !(10>8));
//	return 0;
//}

#include <stdio.h>
int main() {
	int res;
	res = 0&&0;
	printf("%d\n",res);
	res = 0&&1;
	printf("%d\n",res);
	res=1&&0;
	printf("%d\n",res);
	res=1&&1;
	printf("%d\n",res);
	
	res=0||0;
	printf("%d\n",res);
	res=0||1;
	printf("%d\n",res);
	res=1||0;
	printf("%d\n",res);
	res=1||1;
	printf("%d\n",res);
	
	printf("%d\n",!res);
}
