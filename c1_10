#include <stdio.h>
int main() {
	int a,b,count=0,sum=0;
	scanf("%d %d",&a ,&b);
	if(a<b){
	for(int i=b-1;i>a;i--){
		printf("%d ",i);
		count++;
		sum+=i;
	}
 }
 else{
 	for(int i=a-1;i>b;i--){
 		printf("%d",i);
 		++count;
 		sum+=i;
	 }
 }
 printf("\n%d와 %d사이 수의 개수는: %d\n",a,b,count);
 printf("%d와 %d사이 수들의 합은:%d",a,b,sum);
}
