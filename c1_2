#include <stdio.h>

int main() {
	printf("오늘 날짜는%s년 %d월 %s일입니다.","2024",6,"15");
	printf("\n현재 시간은 %d시 %s분 %s초입니다.",4,"12","55");
	printf("\n밑변이 3cm,높이가 4cm인 삼각형의 넓이는 %s입니다.","6cm^2");
	printf("\n지름이 9cm인 원의 넓이는 %.3f이고 둘레는 %.2f입니다.",20.25*3.14,9*3.14);
	printf("\n12378m는 %.3fkm입니다.",12.378);
	printf("\n34.536kg은%sg입니다.","34536");
	printf("\n34.5%%는 %d할 %d푼 %d리입니다.",3,4,5);
	printf("\n%d,%o,%x",255,255,255);
	return 0;
	 
}
