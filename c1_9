//문제 5번
#include <stdio.h>
int main() {
	int sum,min=100,input;
	for(int i=0;i<=3;i++){
       scanf("%d",&input);
        if(input %2 !=0){
       	sum += input;
       		if(min>input){
    		min=input;
	   	}
       }
	}
	if(min==100){ //sum == 0
	printf("-1");
}
else{
	printf("%d\n",sum);
	printf("%d\n",min);
} 
