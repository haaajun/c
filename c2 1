#include <stdio.h>
int main() {
	int n;
	int arg[1000];
	int min=1000,max=0;
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		scanf("%d",&arg[i]);
		if(arg[i]<min){
			min=arg[i];
		}
		if(arg[i]>max){
			max=arg[i];
		}
	}
	printf("%d\n",min);
	printf("%d",max);
}
